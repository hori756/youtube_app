<h1>検索内容</h1>
<%#= link_to '検索', youtube_search_path %>
<%= form_with(model: @yourube, url: youtube_search_path, local: true) do |form| %>
  <div class="field">
    <%= form.label :keyword, "キーワード" %>
    <%= form.text_field :keyword %>
  </div>

  <div class="actions">
    <%= form.submit "検索" %>
  </div>
<% end %>